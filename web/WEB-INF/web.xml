<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>webapp.login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signUp</servlet-name>
        <servlet-class>webapp.signUp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addClass</servlet-name>
        <servlet-class>webapp.addClass</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>addClass</servlet-name>
        <url-pattern>/addClass</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>editUserInfo</servlet-name>
        <servlet-class>webapp.editUserInfo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>editUserInfo</servlet-name>
        <url-pattern>/editUserInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>viewClass</servlet-name>
        <servlet-class>webapp.viewClass</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>viewClass</servlet-name>
        <url-pattern>/viewClass</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>editUserPostInfo</servlet-name>
        <servlet-class>webapp.editUserPostInfo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>editUserPostInfo</servlet-name>
        <url-pattern>/editUserPostInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addAssignment</servlet-name>
        <servlet-class>webapp.addAssignment</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>addAssignment</servlet-name>
        <url-pattern>/addAssignment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addQuestion</servlet-name>
        <servlet-class>webapp.addQuestion</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>addQuestion</servlet-name>
        <url-pattern>/addQuestion</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>enrollStudent</servlet-name>
        <servlet-class>webapp.enrollStudent</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>enrollStudent</servlet-name>
        <url-pattern>/enrollStudent</url-pattern>
    </servlet-mapping>

</web-app>
